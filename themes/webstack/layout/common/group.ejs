<div class="category-section">
<h4 class="text-gray"><i class="linecons-tag" style="margin-right: 7px;" id="<%= e.name.replace(/[\s]+/g, '-') %>"></i><%= e.name %></h4>
<div class="row bookmark-grid">
  <% menus.forEach(function(menu, index) { %>
  <div class="col-sm-3 bookmark-item" data-index="<%= index %>">
    <div class="xe-widget xe-conversations box2 label-info bookmark-card" onclick="window.open('<%= url_for(menu.url) %>', '_blank')" data-toggle="tooltip" data-placement="bottom" title="<%= menu.description %>" data-original-title="<%= url_for(menu.url) %>" data-category="<%= e.name %>" data-name="<%= menu.name %>">
      <div class="xe-comment-entry">
        <a class="xe-user-img">
          <img data-src="<%= url_for(menu.img) %>" class="lozad img-circle" width="40" alt="<%= menu.name %>">
        </a>
        <div class="xe-comment">
          <a href="#" class="xe-user-name overflowClip_1">
            <strong><%= menu.name %></strong>
          </a>
          <p class="overflowClip_2"><%= menu.description %></p>
        </div>
      </div>
    </div>
  </div>
  <% }) %>
</div>
</div>
<br />
